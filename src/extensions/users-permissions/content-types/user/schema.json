{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 6,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": true,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "fullName": {
      "type": "string",
      "required": true,
      "minLength": 3,
      "maxLength": 32
    },
    "phone": {
      "type": "string",
      "required": true,
      "minLength": 5,
      "maxLength": 15
    },
    "companyName": {
      "type": "string"
    },
    "companyWebsite": {
      "type": "string"
    },
    "message": {
      "type": "text"
    },
    "businessDirection": {
      "type": "enumeration",
      "enum": [
        "FBA, FBW, Private Label",
        "Online arbitration",
        "Wholesale",
        "FBM"
      ],
      "required": true
    }
  }
}
