{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "companyName": {
      "type": "string",
      "required": true
    },
    "orderType": {
      "type": "enumeration",
      "enum": [
        "inbound",
        "removal"
      ],
      "required": true
    },
    "warehouseAddress": {
      "type": "enumeration",
      "enum": [
        "address1",
        "address2"
      ],
      "required": true
    },
    "tracker": {
      "type": "string"
    },
    "totalPrice": {
      "type": "decimal"
    },
    "accept": {
      "type": "boolean"
    },
    "rejectionReason": {
      "type": "text"
    },
    "orderDate": {
      "type": "date"
    },
    "comments": {
      "type": "text"
    },
    "totalMasterBoxes": {
      "type": "integer"
    },
    "products": {
      "displayName": "products",
      "type": "component",
      "repeatable": true,
      "component": "products.products"
    },
    "mailFor": {
      "type": "email",
      "unique": false,
      "required": true
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "orders"
    }
  }
}
